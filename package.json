{
  "name": "gnerate",
  "version": "0.0.7",
  "description": "Command-line tool for generating files",
  "main": "./dist/index.js",
  "repository": {
    "url": "https://github.com/ganderzz/gnerate",
    "type": "git"
  },
  "bin": "./dist/index.js",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "test": "./node_modules/.bin/jest",
    "generate": "./node_modules/typescript/bin/tsc & node ./dist/index.js component example/test.js --config=example/config.js --param=3 --keysCanBeAnything=additionalParam"
  },
  "keywords": [
    "gnerate",
    "react",
    "redux",
    "terminal",
    "commandline"
  ],
  "author": "Dylan Paulus",
  "license": "ISC",
  "dependencies": {
    "nunjucks": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.26",
    "@types/nunjucks": "^0.0.33",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.4.0",
    "jest": "^21.0.2",
    "prettier": "^1.6.1",
    "ts-jest": "^21.0.0",
    "typescript": "^2.5.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
